<h2>Contact us</h2>


<%= form_for :contact, url: contacts_path, local: true, data: { turbo: false } do |f| %>
  <p>
    <%= f.label :email %><br/>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :message %><br/>
    <%= f.text_area :message %>
  </p>
  <p>
    <%= f.submit 'Send message', data: { disable_with: false } %>
  </p>
<% end %>
